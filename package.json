    "node-mocks-http": "1.8.1",
    "nodemon": "2.0.4",
    "prettier": "^2.0.5",
    "prettier-plugin-import-sort": "^0.0.4",
    "redux-saga-test-plan": "4.0.0-rc.3",
    "sql.js": "1.3.0",
    "supertest": "4.0.2",
    "ts-jest": "26.1.0",
    "typescript": "3.9.5",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-node-externals": "1.7.2"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 100
  },
  "importSort": {
    ".ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts(x)": "prettier --write"
  },
  "eslint": {
    "parserOptions": {
      "ecmaVersion": 2019
    },
    "env": {
      "es6": true
    },
    "extends": [
      "plugin:jest/recommended",
      "plugin:jest/style"
    ],
    "plugins": [
      "react-hooks",
      "jest"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  }
}
