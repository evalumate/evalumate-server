    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-markdown": "4.3.1",
    "react-modal-hook": "3.0.0",
    "react-redux": "7.2.0",
    "react-transition-group": "4.4.1",
    "recharts": "1.8.5",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-saga": "1.1.3",
    "redux-thunk": "2.3.0",
    "reflect-metadata": "0.1.13",
    "reselect": "4.0.0",
    "sqlite3": "4.2.0",
    "styled-components": "5.1.1",
    "svg-captcha": "1.4.0",
    "svg-inline-react": "3.2.0",
    "swr": "0.2.3",
    "typeorm": "0.2.25",
    "typesafe-actions": "5.1.0",
